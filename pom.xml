<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>3.2.2</version>
		<relativePath/> <!-- lookup parent from repository -->
	</parent>
	<groupId>com.altinntech</groupId>
	<artifactId>clicksave</artifactId>
	<version>1.1.5</version>
	<name>clicksave</name>
	<description>ClickSave is an Object-Relational Mapping (ORM) system written in Java specifically designed for interacting with ClickHouse databases. It provides a seamless interface for Java applications to interact with ClickHouse databases, allowing for efficient data manipulation, retrieval, and management. ClickSave simplifies the integration of ClickHouse databases into Java-based projects, offering developers a powerful toolset to work with ClickHouse databases within their applications.</description>
	<developers>
		<developer>
			<name>Fyodor Plotnikov</name>
			<email>plotnikov.feodor@yandex.ru</email>
			<organization>AltInnTech</organization>
			<organizationUrl>https://altinntech.com/en/</organizationUrl>
		</developer>
		<developer>
			<name>Sergey Tsarev</name>
			<email>b1rk1f23@gmail.com</email>
			<organization>AltInnTech</organization>
			<organizationUrl>https://altinntech.com/en/</organizationUrl>
		</developer>
		<developer>
			<name>Anton Volkov</name>
			<email>anton.wolches@gmail.com</email>
			<organization>AltInnTech</organization>
			<organizationUrl>https://altinntech.com/en/</organizationUrl>
		</developer>
	</developers>
	<properties>
		<java.version>21</java.version>
		<spring.boot.version>3.2.1</spring.boot.version>
		<apache.camel.version>4.3.0</apache.camel.version>
	</properties>
	<dependencyManagement>
		<dependencies>
			<!-- Spring Boot BOM -->
			<dependency>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-dependencies</artifactId>
				<version>${spring.boot.version}</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>
		</dependencies>
	</dependencyManagement>

	<distributionManagement>
		<repository>
			<id>github</id>
			<name>GitHub Altinntech Apache Maven Packages</name>
			<url>https://maven.pkg.github.com/altinntech/Clicksave</url>
		</repository>
	</distributionManagement>

	<dependencies>
		<dependency>
			<groupId>com.google.code.gson</groupId>
			<artifactId>gson</artifactId>
			<version>2.8.8</version>
		</dependency>
		<dependency>
			<groupId>org.projectlombok</groupId>
			<artifactId>lombok</artifactId>
			<optional>true</optional>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter</artifactId>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-aop</artifactId>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-test</artifactId>
			<scope>test</scope>
		</dependency>

		<dependency>
			<groupId>cc.blynk.clickhouse</groupId>
			<artifactId>clickhouse4j</artifactId>
			<version>1.4.4</version>
		</dependency>
		<dependency>
			<groupId>org.reflections</groupId>
			<artifactId>reflections</artifactId>
			<version>0.9.12</version>
		</dependency>
		<dependency>
			<groupId>org.thepavel</groupId>
			<artifactId>spring-icomponent</artifactId>
			<version>1.0.8</version>
		</dependency>
		<dependency>
			<groupId>org.junit.jupiter</groupId>
			<artifactId>junit-jupiter-engine</artifactId>
			<version>5.10.0</version>
			<scope>test</scope>
		</dependency>
	</dependencies>


	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.8.1</version>
				<configuration>
					<source>21</source>
					<target>21</target>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-deploy-plugin</artifactId>
				<version>3.0.0-M1</version>
				<configuration>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-source-plugin</artifactId>
				<version>3.2.1</version>
				<executions>
					<execution>
						<id>attach-sources</id>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

</project>